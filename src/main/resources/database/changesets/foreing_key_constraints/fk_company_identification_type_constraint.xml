<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet author="oscar jimenez carmona" id="55616586">
        <preConditions onFail="CONTINUE" onError="CONTINUE">
            <not>
                <foreignKeyConstraintExists foreignKeyName="fk_company_identification_type_constraint"/>
            </not>
        </preConditions>
        <addForeignKeyConstraint baseColumnNames="identification_type"
                                 baseTableName="companies"
                                 constraintName="fk_company_identification_type_constraint"
                                 onDelete="CASCADE"
                                 onUpdate="RESTRICT"
                                 referencedColumnNames="id"
                                 referencedTableName="identification_types"/>
    </changeSet>
</databaseChangeLog>
