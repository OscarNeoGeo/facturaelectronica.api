<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet author="oscar jimenez carmona" id="56415468">
        <preConditions onFail="CONTINUE" onError="CONTINUE">
            <not>
                <foreignKeyConstraintExists foreignKeyName="fk_client_district_constraint"/>
            </not>
        </preConditions>
        <addForeignKeyConstraint baseColumnNames="district"
                                 baseTableName="clients"
                                 constraintName="fk_client_district_constraint"
                                 onDelete="CASCADE"
                                 onUpdate="RESTRICT"
                                 referencedColumnNames="id"
                                 referencedTableName="districts"/>
    </changeSet>
</databaseChangeLog>
